    "def umcp_can(xx,n,f):\n",
    "    #import file\n",
    "    x=pd.read_csv(xx+'_MXFiltered.tsv',sep='\\t')\n",
    "    x.sort_values('SpecEValue',inplace=True)\n",
    "    if n=='4SU':\n",
    "        x['Ribose']=[int(('+226.059' in i) or ('+57.021+169.038' in i)) for i in list(x['Peptide'])]\n",
    "        x['Peptide']=x['Peptide'].str.replace('+','',regex=True).str.replace('94.017','.',regex=True).str.replace('226.059','.',regex=True).str.replace('57.02136.995','.',regex=True).str.replace('57.021169.038','.',regex=True).str.replace('57.021','',regex=True)\n",
    "    elif n=='6SG':\n",
    "        x['Ribose']=[int(('+265.081' in i) or ('+57.021+208.060' in i)) for i in list(x['Peptide'])]\n",
    "        x['Peptide']=x['Peptide'].str.replace('+','',regex=True).str.replace('133.039','.',regex=True).str.replace('57.02176.017','.',regex=True).str.replace('265.081','.',regex=True).str.replace('57.021208.060','.',regex=True).str.replace('57.021','',regex=True)\n",
    "    else:\n",
    "        pass\n",
