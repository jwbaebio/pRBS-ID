    "#### 5. Further-processing for pRBS-ID closed search with precursor mass correction"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def umcp(xx,n,f):\n",
    "    #count specid in mgf file\n",
    "    N=0\n",
    "    with open('mgf_duplication/'+xx.split('/')[1]+'_MXFiltered.mgf') as infile:\n",
    "        for line in infile:\n",
    "            if 'BEGIN IONS' in line:\n",
    "                N+=1\n",
    "    \n",
    "    #import file\n",
    "    x=pd.read_csv(xx+'_MXFiltered_RBDuplicated.tsv',sep='\\t')\n",
    "    x.sort_values('SpecEValue',inplace=True)\n",
    "    xs=list(x['SpecID'])\n",
    "    x['SpecInt']=[int(i.split('=')[1]) for i in xs]\n",
    "    x['Ribose']=[1-int(int(i.split('=')[1])<N) for i in xs]\n",
    "    for i in x.index:\n",
    "        if x.loc[i,'SpecInt']>=N:\n",
    "            x.loc[i,'SpecID']='index='+str(x.loc[i,'SpecInt']-N)\n",
    "    if n=='4SU':\n",
    "        x['Peptide']=x['Peptide'].str.replace('+','',regex=True).str.replace('94.017','.',regex=True).str.replace('57.02136.995','.',regex=True).str.replace('57.021','',regex=True)\n",
    "    elif n=='6SG':\n",
    "        x['Peptide']=x['Peptide'].str.replace('+','',regex=True).str.replace('133.039','.',regex=True).str.replace('57.02176.017','.',regex=True).str.replace('57.021','',regex=True)\n",
    "    else:\n",
    "        pass\n",
    "    \n",
    "    #10ppm filter\n",
    "    x11=x.loc[(x['QValue'] == 0)]\n",
    "    x11.drop_duplicates(['ScanNum'],keep='first',inplace=True)\n",
    "    ppm=np.mean(x11['PrecursorError(ppm)'])\n",
    "    x=x.loc[(x['PrecursorError(ppm)'] >= ppm-5)]\n",
    "    x=x.loc[(x['PrecursorError(ppm)'] <= ppm+5)]\n",
    "    \n",
    "    #Remove duplicates per scan & unlocalized RBS-PSMs (Including C-term RBS) & Non-RBS-PSMs \n",
    "    xs=list(x['SpecInt'])\n",
    "    x['Sbool1']=[xs.count(i) for i in xs]\n",
    "    x['Sbool2']=[int('.'!=i[-1])  for i in list(x['Peptide'])]\n",
    "    x['Rbool']=[int(('.' in i))  for i in list(x['Peptide'])]\n",
    "    x.drop_duplicates(['ScanNum'],keep='first',inplace=True)\n",
    "    x=x.loc[x['Rbool']==1].loc[x['Sbool1']==1].loc[x['Sbool2']==1]\n",
    "    del x['Rbool']\n",
    "    del x['Sbool1']\n",
    "    del x['Sbool2']\n",
    "    del x['SpecInt']\n",
    "    x['PepSeq']=[x.loc[i,'Peptide'].replace('.','') for i in x.index]\n",
    "    \n",
    "    #Assign UMC\n",
    "    umc=pd.read_csv(xx+'_UMC_MSMS_MX.txt',sep='\\t')\n",
    "    x['UMC']=[umc.loc[int(x.loc[i,'SpecID'].split('=')[1]),'UmcIndex'] for i in x.index]\n",
    "\n",
    "    #Single PSM per UMC & localization score\n",
    "    keep1,keep2=[],[]\n",
    "    x['PepScore']=[-np.log10(x.loc[i,'SpecEValue']) for i in x.index]\n",
    "    x['KeepScore']=[0]*x.shape[0]\n",
    "    lc,kc=[],[]\n",
    "    for i in x.index:\n",
    "        try:\n",
    "            kc.append(sum(x.loc[x['UMC']==x.loc[i,'UMC']].loc[x['PepSeq']==x.loc[i,'PepSeq']]['PepScore'])/sum(x.loc[x['UMC']==x.loc[i,'UMC']]['PepScore']))\n",
    "        except:\n",
    "            kc.append(0)\n",
    "    x['KeepScore']=kc\n",
    "    for i in x.index:\n",
    "        if x.loc[i,'KeepScore']==max(x.loc[x['UMC']==x.loc[i,'UMC']]['KeepScore']):\n",
    "            keep1.append(i)\n",
    "        else:\n",
    "            pass\n",
    "    x=x.loc[keep1]\n",
    "    x['LocScore']=[0]*x.shape[0]\n",
    "    for i in x.index:\n",
    "        try:\n",
    "            lc.append(sum(x.loc[x['UMC']==x.loc[i,'UMC']].loc[x['Peptide']==x.loc[i,'Peptide']]['PepScore'])/sum(x.loc[x['UMC']==x.loc[i,'UMC']]['PepScore']))\n",
    "        except:\n",
    "            lc.append(0)\n",
    "    x['LocScore']=lc\n",
    "    for i in x.index:\n",
    "        if x.loc[i,'LocScore']==max(x.loc[x['UMC']==x.loc[i,'UMC']]['LocScore']):\n",
    "            keep2.append(i)\n",
    "        else:\n",
    "            pass\n",
    "    x=x.loc[keep2]\n",
    "    del x['KeepScore']\n",
    "    x.drop_duplicates(['UMC'],keep='first',inplace=True)\n",
    "\n",
    "    #UMC-level FDR 1% cutoff, remove decoy\n",
    "    x.set_index('SpecID',inplace=True)\n",
    "    x['UMC_QValue']=[len([j for j in list(x['Protein'])[:i+1] if 'XXX' in j])/(i+1) for i in range(x.shape[0])]\n",
    "    c=0\n",
    "    for i in range(x.shape[0]):\n",
    "        if x.iloc[c,-1]>0.01:\n",
    "            break\n",
    "        c+=1\n",
    "    x=x.iloc[:c,:]\n",
    "    x['Decoy']=[int('XXX' in x.loc[i,'Protein']) for i in x.index]\n",
    "    x=x.loc[x['Decoy']==0]\n",
    "\n",
    "    x['Mod']=[str(x.loc[i,'Peptide'].find('.'))+x.loc[i,'Peptide'][x.loc[i,'Peptide'].find('.')-1] for i in x.index]\n",
    "    prot,site=[],[]\n",
    "    for i in list(x.index):\n",
    "        s,p='',''\n",
    "        for j in range(x.loc[i,'Protein'].count(';')+1):\n",
    "            s+=x.loc[i,'Protein'].split(';')[j].split('(')[0]+'.'+x.loc[i,'Mod'][-1]+str(str(f.loc[x.loc[i,'Protein'].split(';')[j].split('(')[0],'Sequence']).find(x.loc[i,'PepSeq'])+int(x.loc[i,'Mod'][:-1]))\n",
    "            p+=x.loc[i,'Protein'].split(';')[j].split('(')[0]\n",
    "            if j!=x.loc[i,'Protein'].count(';'):\n",
    "                s+=';'\n",
    "                p+=';'\n",
    "        site.append(s)\n",
    "        prot.append(p)\n",
    "    x['ProteinGroup']=prot\n",
    "    x['RBS']=site\n",
    "\n",
    "    #UMC LFQ\n",
    "    lfq=pd.read_csv(xx+'_UMC.txt',sep='\\t')\n",
    "    x['Intensity']=[lfq.loc[x.loc[i,'UMC'],'MostAbundant'] for i in x.index]\n",
    "\n",
    "    #columns, report, save\n",
    "    x=x[['ScanNum','UMC','ProteinGroup','PepSeq','Peptide','Mod','RBS','Ribose','PepScore','LocScore','UMC_QValue','Intensity']]\n",
    "    print('Proteins\\tPeptides\\tRBSs')\n",
    "    print(str(len(list(set(list(x['ProteinGroup'])))))+'\\t\\t'+str(len(list(set(list(x['PepSeq'])))))+'\\t\\t'+str(len(list(set(list(x['RBS'])))))+'\\n')\n",
    "    x.to_csv(xx+'_mpemmr-msgf_RBDup_umcprocessed.tsv',sep='\\t')\n",
    "    return x\n",
    "\n",
    "a1=umcp('mRBS_dTamicon30K_4SU_HCD_half_1','4SU',f)\n",
    "a2=umcp('mRBS_dTamicon30K_4SU_HCD_half_2','4SU',f)\n",
    "b1=umcp('mRBS_dTamicon30K_6SG_HCD_half_1','6SG',f)\n",
    "b2=umcp('mRBS_dTamicon30K_6SG_HCD_half_2','6SG',f)"
   ]
  },
