    "#### 4. MXFiltered.mgf file duplication"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "filenames = ['mRBS_dTamicon30K_4SU_HCD_half_1_MXFiltered','mRBS_dTamicon30K_4SU_HCD_half_2_MXFiltered',\n",
    "             'mRBS_dTamicon30K_6SG_HCD_half_1_MXFiltered','mRBS_dTamicon30K_6SG_HCD_half_2_MXFiltered',\n",
    "            ]\n",
    "for fname in filenames:\n",
    "    fnames=[fname+'.mgf',fname+'_RBRemoved.mgf']\n",
    "    pl,cl=[],[]\n",
    "    pn=0\n",
    "    with open(fnames[0]) as infile:\n",
    "        for line in infile:\n",
    "            if 'PEPMASS=' in line:\n",
    "                pl.append(eval(line.split(' ')[0].split('=')[1]))\n",
    "            elif 'CHARGE=' in line:\n",
    "                cl.append(eval(line.split('+')[0].split('=')[1]))\n",
    "            else:\n",
    "                pass    \n",
    "    with open(fnames[1], 'w') as outfile:\n",
    "        with open(fnames[0]) as infile:\n",
    "            for line in infile:\n",
    "                if 'PEPMASS=' in line:\n",
    "                    line1=line.split(' ')\n",
    "                    outfile.write('PEPMASS='+str(round(pl[pn]-132.0422568/cl[pn],5))+' '+line1[1])\n",
    "                    pn+=1\n",
    "                else:\n",
    "                    outfile.write(line)\n",
    "    with open(fname+'_RBDuplicated.mgf', 'w') as outfile:\n",
    "        for fname1 in fnames:\n",
    "            with open(fname1) as infile:\n",
    "                for line in infile:\n",
    "                    outfile.write(line)\n",
    "    "
   ]
